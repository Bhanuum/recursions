#include <iostream>

using namespace std;
int fib(int *a,int n)
{
    if(n==0||n==1)return true;
    if(a[0]>a[1])return false;
    bool s=fib(a+1,n-1);
    if(s)return true;
    else return false;
    
    
    
}

int main()
{
    int n;
    cin>>n;
    int a[4]={1,2,3,8};
    int p=fib(a,n);
    cout<<p;
}
